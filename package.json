{
    "name": "whet",
    "version": "0.1.0",
    "description": "NodeJS based assets management and project tooling library.",
    "scripts": {
        "devinit": "npx dts2hx commander pino-pretty minimatch --modular --noLibWrap",
        "build": "npx haxe build.hxml",
        "test": "node --test \"test/**/*.test.mjs\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Antriel/whet.git"
    },
    "files": [
        "bin"
    ],
    "bin": {
        "whet": "bin/whet.js"
    },
    "main": "bin/whet.js",
    "type": "module",
    "author": "Peter Achberger",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Antriel/whet/issues"
    },
    "homepage": "https://github.com/Antriel/whet#readme",
    "dependencies": {
        "commander": "^14.0.3",
        "minimatch": "^10.2.2",
        "pino-pretty": "^13.1.3"
    },
    "devDependencies": {
        "dts2hx": "^0.20.0",
        "lix": "^16.0.2"
    }
}
